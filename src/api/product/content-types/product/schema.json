{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "required": true,
      "maxLength": 100,
      "minLength": 3
    },
    "Description": {
      "type": "text",
      "required": true,
      "maxLength": 110,
      "minLength": 20
    },
    "Title": {
      "type": "string",
      "required": true,
      "maxLength": 20,
      "minLength": 3
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "T-shirt",
        "Mug",
        "Sticker",
        "Pants"
      ],
      "default": "T-shirt",
      "required": true
    },
    "Size": {
      "type": "string"
    },
    "Colour": {
      "type": "enumeration",
      "enum": [
        "red",
        "blue",
        "black",
        "white",
        "green ",
        "yellow"
      ]
    },
    "Price": {
      "type": "integer",
      "required": true
    },
    "AvilableQTY": {
      "type": "integer",
      "required": true
    }
  }
}
